#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/caixukun/workspace/digital-lotus-cli/node_modules/.pnpm/digital-lotus-cli@0.0.1/node_modules/digital-lotus-cli/bin/node_modules:/Users/caixukun/workspace/digital-lotus-cli/node_modules/.pnpm/digital-lotus-cli@0.0.1/node_modules/digital-lotus-cli/node_modules:/Users/caixukun/workspace/digital-lotus-cli/node_modules/.pnpm/digital-lotus-cli@0.0.1/node_modules:/Users/caixukun/workspace/digital-lotus-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/caixukun/workspace/digital-lotus-cli/node_modules/.pnpm/digital-lotus-cli@0.0.1/node_modules/digital-lotus-cli/bin/node_modules:/Users/caixukun/workspace/digital-lotus-cli/node_modules/.pnpm/digital-lotus-cli@0.0.1/node_modules/digital-lotus-cli/node_modules:/Users/caixukun/workspace/digital-lotus-cli/node_modules/.pnpm/digital-lotus-cli@0.0.1/node_modules:/Users/caixukun/workspace/digital-lotus-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../digital-lotus-cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../digital-lotus-cli/bin/cli.js" "$@"
fi
